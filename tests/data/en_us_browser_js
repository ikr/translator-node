var off_0 = 0;
var pf_0 = {
"one" : function(d){
var r = "";
r += "a room";
return r;
},
"other" : function(d){
var r = "";
r += "" + (function(){ var x = k_1 - off_0;
if( isNaN(x) ){
throw new Error("MessageFormat: `"+lastkey_1+"` isnt a number.");
}
return x;
})() + " rooms";
return r;
}
};
if ( pf_0[ k_1 + "" ] ) {
r += pf_0[ k_1 + "" ]( d );
}
else {
r += (pf_0[ MessageFormat.locale["en"]( k_1 - off_0 ) ] || pf_0[ "other" ] )( d );
}
r += " is confirmed,\nand is assigned a reference number ";
if(!d){
throw new Error("MessageFormat: No data passed to function.");
}
r += d["RESERVATION_ID"];
r += ".\n\nCheck-in date:  ";
